    RowBox[{"2", "Pi", "*", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Pochhammer", "[", 
           RowBox[{
            RowBox[{"1", "/", "2"}], ",", "k"}], "]"}], "/", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"2", "k"}], ")"}], "!"}]}], "/", 
         RowBox[{"Pochhammer", "[", 
          RowBox[{
           RowBox[{"2", "/", "2"}], ",", "k"}], "]"}]}], "*", 
        RowBox[{
         RowBox[{"\[Epsilon]", "^", 
          RowBox[{"(", 
           RowBox[{"-", 
            RowBox[{"(", 
             RowBox[{"\[Nu]Fixed", "-", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"2", "k"}], "+", "2"}], ")"}]}], ")"}]}], ")"}]}], 
         "/", 
         RowBox[{"(", 
          RowBox[{"\[Nu]Fixed", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "k"}], "+", "2"}], ")"}]}], ")"}]}], "*", 
        RowBox[{"Nest", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Laplacian", "[", 
            RowBox[{"#", ",", 
             RowBox[{"{", 
              RowBox[{"x1", ",", "x2"}], "}"}]}], "]"}], "&"}], ",", 
          RowBox[{"g0", "[", 
           RowBox[{"x1", ",", "x2"}], "]"}], ",", "k"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "0", ",", "1"}], "}"}]}], "]"}]}]}]}], 
