Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"SEMOpEvaluatedSkyrmionEpsilon", "[", 
    RowBox[{"x1_", ",", "x2_", ",", "\[Lambda]_", ",", "\[Epsilon]_"}], "]"}],
    "=", 
   RowBox[{
    RowBox[{"SEMOp", "[", 
     RowBox[{
      RowBox[{"{", 
