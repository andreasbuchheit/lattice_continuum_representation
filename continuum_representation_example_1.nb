            RowBox[{"s", "/", 
             RowBox[{"(", 
              RowBox[{"2", "*", 
               RowBox[{"1", "/", "\[Lambda]"}]}], ")"}]}]}], ",", "x2", ",", 
           "\[Lambda]"}], "]"}], "+", 
         RowBox[{"Sy", "[", 
          RowBox[{
           RowBox[{"x1", "+", 
            RowBox[{"s", "/", 
             RowBox[{"(", 
              RowBox[{"2", "*", 
               RowBox[{"1", "/", "\[Lambda]"}]}], ")"}]}]}], ",", "x2", ",", 
           "\[Lambda]"}], "]"}], "-", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], ")"}]}], "/", 
      RowBox[{"Norm", "[", 
       RowBox[{
        RowBox[{"Sy", "[", 
         RowBox[{
          RowBox[{"x1", "-", 
           RowBox[{"s", "/", 
            RowBox[{"(", 
             RowBox[{"2", "*", 
              RowBox[{"1", "/", "\[Lambda]"}]}], ")"}]}]}], ",", "x2", ",", 
          "\[Lambda]"}], "]"}], "+", 
        RowBox[{"Sy", "[", 
         RowBox[{
          RowBox[{"x1", "+", 
           RowBox[{"s", "/", 
            RowBox[{"(", 
             RowBox[{"2", "*", 
              RowBox[{"1", "/", "\[Lambda]"}]}], ")"}]}]}], ",", "x2", ",", 
          "\[Lambda]"}], "]"}], "-", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "]"}]}], "/.", 
     RowBox[{"Abs", "->", "RealAbs"}]}], "//", "Simplify"}]}], ";"}]], "Input",\

