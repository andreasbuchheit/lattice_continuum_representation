  RowBox[{
   RowBox[{"g0", "[", 
    RowBox[{"x1_", ",", "x2_"}], "]"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0", ",", "1"}], "}"}], ".", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Sy", "[", 
          RowBox[{
